pipeline{
    agents any
        stages{
            stage{
                steps{
                    script{
                        a=10
                        b=20
                        if (a>b) {
                            println "a: ${a} is Big"
                        }
                        else{
                            println "b: ${b} is Big"
                        }

                }
            }
            
        }
    }
}
